  <body style="padding-top: 0px;">
    <div class="container-fluid px-3">
  <div class="row min-vh-100">
    <div class="col-md-8 col-lg-6 col-xl-5 d-flex align-items-center">
      <div class="w-100 py-5 px-md-5 px-xl-6 position-relative">
        <div class="mb-5">
          <h2>Welcome back</h2>
        </div>
        <%= simple_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
<div class="form-inputs">
<%= f.input :email,
            required: false,
            autofocus: true,
            input_html: { autocomplete: "email" } %>
<%= f.input :password,
            required: false,
            input_html: { autocomplete: "current-password" } %>
<%= f.input :remember_me, as: :boolean if devise_mapping.rememberable? %>
</div>
<div class="form-actions">
<%= f.button :submit, "Log in" %>
</div>
<% end %>
<%= render "devise/shared/links" %><a href="index.html" class="close-absolute mr-md-5 mr-xl-6 pt-5">
          <svg class="svg-icon w-3rem h-3rem">
            <use xlink:href="#close-1"> </use>
          </svg></a>
      </div>
    </div>
    <div class="col-md-4 col-lg-6 col-xl-7 d-none d-md-block">
      <!-- Image-->
      <div style="background-image: url(https://res.cloudinary.com/danxfwhmp/image/upload/v1579347073/child-613197_pjguvq.jpg);" class="bg-cover h-100 mr-n3"></div>
    </div>
  </div>
</div>
</body>
